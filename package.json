{
  "name": "view-showcase",
  "version": "5.0.4",
  "description": "Show custom elements in action",
  "main": "main/index.js",
  "source": "source/index.js",
  "scripts": {
    "clear": "rm -rf dist/*",
    "start": "npm run clear && parcel showcase/index.html --public-url /showcase",
    "build": "npm run clear && NODE_ENV=production parcel build showcase/index.html --public-url /showcase",
    "proto1": "parcel proto/index.html",
    "proto2": "parcel build proto/index.html --detailed-report",
    "staging": "bash ./scripts/deploy.sh staging",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epha/view-showcase.git"
  },
  "author": "Marco Egbring",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/epha/view-showcase/issues"
  },
  "alias": {
    "view-units": "../view-units",
    "view-types": "../view-types",
    "view-utils": "../view-utils",
    "view-store": "../view-store"
  },
  "homepage": "https://github.com/epha/view-showcase#readme",
  "dependencies": {
    "core-js": "^3.1.4",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.1",
    "object-path-immutable": "^3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1"
  }
}
