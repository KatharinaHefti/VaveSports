  },
  "homepage": "https://github.com/epha/view-showcase#readme",
  "dependencies": {
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.3",
    "object-path-immutable": "^3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "lit-html": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
