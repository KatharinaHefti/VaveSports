{
  "name": "view-showcase",
  "version": "5.0.9",
  "description": "Show custom elements in action",
  "main": "main/index.js",
  "source": "source/index.js",
  "scripts": {
    "prestart": "rm -rf dist/* && rm -rf .cache",
    "start": "parcel showcase/index.html --public-url /showcase",
    "prebuild": "rm -rf dist/* && rm -rf .cache",
    "build": "NODE_ENV=production parcel build showcase/index.html --public-url /showcase  --detailed-report --experimental-scope-hoisting",
    "staging": "bash ./scripts/deploy.sh staging",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epha/view-showcase.git"
  },
  "alias": {
    "view-units": "../view-units",
    "view-types": "../view-types"
  },
  "author": "Marco Egbring",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/epha/view-showcase/issues"
  },
  "homepage": "https://github.com/epha/view-showcase#readme",
  "dependencies": {
    "core-js": "^3.2.1",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "lodash": "^4.17.15",
    "object-path-immutable": "^3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "cssnano": "^4.1.10",
    "htmlnano": "^0.2.4",
    "marked": "^0.7.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1"
  }
}
